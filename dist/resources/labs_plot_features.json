{
	"templates":{
		"views":{
			"PlotLinearWithFctPlotFeatureTemplate":{
				"type":"VBox",
				"settings":{},
				"state":{
					"lab_features":["MathJS"],
					"items":[
						"{{devapt-template-id}}_plot",				
						"{{devapt-template-id}}_fn",
						"{{devapt-template-id}}_pretty",
						"{{devapt-template-id}}_buttons"
					]
				},
				"children":{
					"{{devapt-template-id}}_plot":{
						"type":"function_plot",
						"settings":{
							"style":{
								"width":"100%",
								"height":"100%"
							}
						},
						"state":{
							"plot_type":"linear",
							"plot_method":"polyline",
							"plot_inputs_selectors":["{{devapt-template-id}}_fn_input"],
							"derivative":"true"
						},
						"children":{}
					},
					"{{devapt-template-id}}_fn":{
						"type":"container",
						"settings":{},
						"state":{
							"items":[
								{"view":"{{devapt-template-id}}_fn_label"},
								{"view":"{{devapt-template-id}}_fn_input"}
							]
						},
						"children":{
							"{{devapt-template-id}}_fn_label":{
								"type":"math_pretty",
								"settings":{
									"style":{
										"float":"left",
										"transform": "translate(0%, 40%)",
										"min-width":"60px",
										"max-width":"15%"
									}
								},
								"state":{
									"expression":"f(x)="
								}
							},
							"{{devapt-template-id}}_fn_input":{
								"type":"input-field",
								"settings":{
									"style":{
										"float":"right",
										"vertical-align": "middle",
										"min-width":"100px",
										"max-width":"85%"
									}
								},
								"state":{
									"placeholder":"press enter to plot the function",
									"bindings":{
										"streams":[
											{
												"source_stream":"enter",
												"transform":null,
												"target_type":"view",
												"target_selectors":[
													"{{devapt-template-id}}_plot",
													"{{devapt-template-id}}_pretty"
												],
												"target_method":"process",
												"options":{
													"method":{
														"operands":{
															"expr_input_id":"{{devapt-template-id}}_fn_input",
															"prefix":"f(x)="
														}
													}
												}
											}
										]
									}
								},
								"children":{}
							}
						}
					},
					"{{devapt-template-id}}_pretty":{
						"type":"math_pretty",
						"settings":{
							"style":{
								"margin-top":"10px",
								"margin-bottom":"10px"
							}
						},
						"state":{
							"expression":"f(x)="
						},
						"children":{}
					},
					"{{devapt-template-id}}_buttons":{
						"type":"container",
						"settings":{
							"class":"button-group"
						},
						"state":{
							"items":[ { "view":"{{devapt-template-id}}_settings_button" }, { "view":"{{devapt-template-id}}_clear_button" } ]
						},
						"children":{}
					},
					"{{devapt-template-id}}_settings_button":{
						"type":"button",
						"settings":{},
						"state":{
							"label":"Options"
						},
						"children":{}
					},
					"{{devapt-template-id}}_clear_button":{
						"type":"button",
						"settings":{},
						"state":{
							"label":"Clear",
							"bindings":{
								"emitter_dom":[
									{
										"source_type":"delegate",
										"source_selector":"button",
										"dom_event":"click",
										"transform":null,
										"target_type":"view",
										"target_selector":"{{devapt-template-id}}_plot",
										"target_method":"clear_functions"
									}
								]
							}
						},
						"children":{}
					}
				}
			},
