			"commands":{
				"abs":{
					"usage":"abs(expression)",
					"operands":[ { "name":"expression", "type":"number" } ],
					"returns":"number",
					"domain":"arithmetic/numerical",
					"description":"Returns the absolute value or vector length of x.",
					"examples":[
						{
							"command":"abs(-67)",
							"result":"67",
							"description":"absolute value of -67 is 67."
						}
					]
				},
				"adj":{
					"usage":"ajd(matrix)",
					"operands":[ { "name":"m", "type":"matrix" } ],
					"returns":"matrix",
					"domain":"linear algebra",
					"description":"Returns the adjunct of matrix m. The inverse of m is equal to adj(m) divided by det(m).",
					"examples":[]
				},
				"and":{
					"usage":"and(expression1, expression2)",
					"operands":[ { "name":"expression1", "type":"boolean" }, { "name":"expression2", "type":"boolean" } ],
					"returns":"boolean",
					"domain":"logical",
					"description":"Logical-and of predicate expressions.",
					"examples":[
						{
							"command":"and(true,false",
							"result":"false",
							"description":"logical operation (true and false)."
						}
					]
				}
			}
		}
	},
